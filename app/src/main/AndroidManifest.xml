<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.ebook">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.NoActionBar">
        <activity android:name=".BookContentActivity"></activity>
        <activity android:name=".ConcernActivity" />
        <activity android:name=".IdealContentActivity" />
        <activity android:name=".signInActivity" />
        <activity android:name=".LoginActivity" />
        <activity android:name=".ReadingPageActivity" />
        <activity android:name=".SearchBookResultActivity" />
        <activity android:name=".LikeClassifyActivity" />
        <activity android:name=".ChangePwdActivity" />
        <activity android:name=".ChangeSignatureActivity" />
        <activity android:name=".ChangeNickNameActivity" />
        <activity android:name=".UserSettingActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>